<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="Bluetooth Settings" >

    <!-- controller -->
    <PreferenceCategory android:title="controller" >
        <ListPreference
            android:defaultValue="0"
            android:entries="@array/bt_dsp_log_ctrl_values"
            android:entryValues="@array/bt_dsp_log_ctrl_values"
            android:key="dsp_log_control"
            android:title="DSP LOG Control" />
        </PreferenceCategory>

        <Preference
            android:key="data_capture"
            android:title="DATA CAPTURE" >
        </Preference>

    <PreferenceCategory android:title="BQB" >
        <Preference
            android:key="controller_bqb_mode"
            android:title="Controller BQB Mode" >
        </Preference>
    </PreferenceCategory>

</PreferenceScreen>
