<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true"  android:state_enabled="true"
          android:drawable="@drawable/edit_storage_pressed" /> <!-- pressed -->
    <item android:state_focused="true"  android:state_enabled="true"
          android:drawable="@drawable/edit_storage_pressed" /> <!-- focused -->
    <item android:state_pressed="false"  android:state_enabled="true"
          android:drawable="@drawable/edit_storage_focus" />
    <item android:state_focused="false"  android:state_enabled="true"
          android:drawable="@drawable/edit_storage_focus" />
    <item android:drawable="@drawable/edit_storage_not_focus" android:state_enabled="false"/> <!-- default -->
</selector>
